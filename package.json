{
	"name": "cloudflare-email-sieve",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "npm run cf-typegen && npm run check && npm run test",
		"test": "vitest --run --typecheck",
		"deploy": "npm run build && npx wrangler deploy",
		"start": "npx wrangler dev",
		"format": "prettier --write .",
		"check": "tsc --noEmit",
		"cf-typegen": "npx wrangler types"
	},
	"devDependencies": {
		"prettier": "^3.6.2",
		"typescript": "^5.9.3",
		"vitest": "^4.0.1",
		"wrangler": "^4.42.2"
	}
}
